{
    "manifest_version": 2,
    "name": "anteater",
    "version": "1.0",
    "description": "Trims all tracking parameters from URLs on copy to the clipboard",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["dist/anteater.js"]
        }
    ],
    "developer": {
        "name": "Scott Jones"
    },
    "permissions": [
        "clipboardWrite"
    ]
}
