{
    "manifest_version": 2,
    "name": "chemo",
    "version": "1.0",
    "description": "trims all tracking parameters from URLs on copy to the clipboard",
    "icons": {
        "48": "icons/48.png"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["clean.js"]
        }
    ],
    "permissions": [
        "clipboardWrite"
    ]
}
